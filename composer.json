{
    "name": "james-n-m/chuck-norris-ipsum",
    "description": "Generate random Chuck Norris jokes and facts placeholder text in your blade views",
    "keywords": [
        "chuck-norris",
        "php",
        "lorem-ipsum",
        "laravel"
    ],
    "homepage": "https://github.com/james-n-m/chuck-norris-ipsum",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "James Moore",
            "email": "jamesm0025@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JamesNM\\ChuckNorrisIpsum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JamesNM\\ChuckNorrisIpsum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
